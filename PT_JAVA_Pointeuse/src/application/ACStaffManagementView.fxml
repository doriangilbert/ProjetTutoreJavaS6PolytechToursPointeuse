<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ACStaffManagementController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Label id="labelStaffManagement" layoutX="223.0" layoutY="29.0" text="Staff management">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <TableView id="tableViewStaffManagement" layoutX="18.0" layoutY="81.0" prefHeight="203.0" prefWidth="604.0">
              <columns>
                  <TableColumn id="tableColumnID" prefWidth="75.0" text="ID" />
                  <TableColumn id="tableColumnFirstName" prefWidth="75.0" text="First Name" />
                  <TableColumn id="tableColumnLastName" prefWidth="75.0" text="Last Name" />
                  <TableColumn id="tableColumnDepartment" prefWidth="75.0" text="Department" />
                  <TableColumn id="tableColumnManage" prefWidth="75.0" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <Button id="buttonCreateEmployee" layoutX="254.0" layoutY="302.0" mnemonicParsing="false" text="Create employee" />
            <Button id="buttonBackToHome" layoutX="265.0" layoutY="355.0" mnemonicParsing="false" text="Back to home" onAction="#handleButtonBackToHome" />
         </children>
    </AnchorPane>
  </children>
</VBox>
