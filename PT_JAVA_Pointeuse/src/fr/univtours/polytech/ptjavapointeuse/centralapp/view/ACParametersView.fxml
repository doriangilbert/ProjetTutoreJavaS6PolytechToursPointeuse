<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.univtours.polytech.ptjavapointeuse.centralapp.controller.ACParametersController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Label id="labelParameters" layoutX="232.0" layoutY="43.0" text="Parameters">
               <font>
                  <Font size="36.0" />
               </font>
            </Label>
            <Label id="labelEmulatorAppIpAddress" layoutX="32.0" layoutY="144.0" text="Emulator app IP address :">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <TextField fx:id="textFieldEmulatorAppIpAddress" layoutX="314.0" layoutY="146.0" prefHeight="31.0" prefWidth="291.0" promptText="Enter the IP address of the emulator app" />
            <Label id="labelEmulatorAppPort" layoutX="96.0" layoutY="222.0" text="Emulator app port :">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <TextField fx:id="textFieldEmulatorAppPort" layoutX="314.0" layoutY="224.0" prefHeight="31.0" prefWidth="291.0" promptText="Enter the port for the emulator app" />
            <Button id="buttonBackToHome" layoutX="265.0" layoutY="323.0" mnemonicParsing="false" onAction="#handleButtonBackToHome" text="Back to home" />
         </children>
    </AnchorPane>
  </children>
</VBox>
