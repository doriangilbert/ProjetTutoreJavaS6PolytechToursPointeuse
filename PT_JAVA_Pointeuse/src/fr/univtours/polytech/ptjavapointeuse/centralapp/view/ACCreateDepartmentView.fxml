<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.univtours.polytech.ptjavapointeuse.centralapp.controller.ACCreateDepartmentController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Label id="labelCreateDepartment" layoutX="220.0" layoutY="79.0" text="Create department">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <TextField fx:id="textFieldDepartmentName" layoutX="272.0" layoutY="185.0" prefHeight="31.0" prefWidth="267.0" />
            <Label id="labelDepartmentName" layoutX="102.0" layoutY="187.0" text="Department name :">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Button id="buttonConfirm" layoutX="283.0" layoutY="264.0" mnemonicParsing="false" onAction="#handleButtonConfirm" text="Confirm" />
            <Button id="buttonBack" layoutX="295.0" layoutY="331.0" mnemonicParsing="false" onAction="#handleButtonBack" text="Back" />
         </children>
    </AnchorPane>
  </children>
</VBox>
