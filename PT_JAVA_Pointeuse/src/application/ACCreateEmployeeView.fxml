<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ACCreateEmployeeController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Label id="labelCreateEmployee" layoutX="231.0" layoutY="34.0" text="Create employee">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <Label id="labelFirstName" layoutX="147.0" layoutY="104.0" text="First name :">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField id="textFieldFirstName" fx:id="TextFirstName" layoutX="256.0" layoutY="102.0" prefHeight="31.0" prefWidth="267.0" />
            <Label id="labelLastName" layoutX="148.0" layoutY="166.0" text="Last name :">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField id="textFieldLastName" fx:id="TextLastName" layoutX="256.0" layoutY="164.0" prefHeight="31.0" prefWidth="267.0" />
            <Label id="labelDepartment" layoutX="131.0" layoutY="225.0" text="Department :">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <ComboBox id="comboBoxDepartment" fx:id="mainDepartmentBox" layoutX="256.0" layoutY="223.0" prefHeight="31.0" prefWidth="267.0" />
            <Button id="buttonConfirm" layoutX="283.0" layoutY="303.0" mnemonicParsing="false" onAction="#handleButtonConfirm" text="Confirm" />
            <Button id="buttonBack" layoutX="295.0" layoutY="355.0" mnemonicParsing="false" onAction="#handleButtonBack" text="Back" />
         </children>
    </AnchorPane>
  </children>
</VBox>
